{
  "notPath": [
    "_ide_helper_models.php",
    "temp.php"
  ],
  "preset": "laravel",
  "rules": {
    "concat_space": {
      "spacing": "one"
    },
    "increment_style": {
      "style": "pre"
    },
    "not_operator_with_successor_space": false,
    "trailing_comma_in_multiline": {
      "after_heredoc": true,
      "elements": ["arguments", "arrays", "match", "parameters"]
    }
  }
}
